#Retrieve all the members of the library along with the books they have access to.

PREFIX ex: <http://example.org/library>
PREFIX foaf: <http://xmlns.com/foaf/0.1/>
PREFIX schema: <http://schema.org/>
PREFIX member: <http://example.org/member#>
PREFIX dcterms: <http://purl.org/dc/terms/>

SELECT ?member ?memberName ?book ?bookTitle
WHERE {
    ?member a foaf:Person ;
        foaf:givenName ?memberName .
    ?book a schema:Book ;
        dcterms:title ?bookTitle .
}